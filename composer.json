{
    "name": "tobischulz/laravel-model-read",
    "description": "Let you set the read state for every model for each user. Get a list of users that has read a post or any other model.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Schulz",
            "email": "tobias@byte.software"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "7.x"
    },
    "autoload": {
        "psr-4": {
            "TobiSchulz\\ModelRead\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TobiSchulz\\ModelRead\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TobiSchulz\\ModelRead\\ModelReadServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
